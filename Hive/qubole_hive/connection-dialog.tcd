<connection-dialog class='qubole_hive'>
  <connection-config>
	<authentication-mode value='PasswordOnly' />
		<authentication-options>
			<option name="Password" default="true" value="api-token" />
		</authentication-options>
    <has-databases value="false" />
    <has-schemas value="true" />
    <server-prompt value="EndPoint:" default="https://api.qubole.com"/>
    <warehouse-prompt value="Cluster Label:" default="hadoop2"/>
  </connection-config>
</connection-dialog>